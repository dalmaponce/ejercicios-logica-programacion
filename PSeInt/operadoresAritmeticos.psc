Proceso operadoresAritmeticos
	Definir a, b, c, d, e, f Como Real;
	
	////	Operadores Aritmeticos
	////	 Suma
	a <- 3 + 4;
	Escribir "Suma: ", a;
	
	////	Resta
	b <- 6 - 2;
	Escribir "Resta: ", b;
	
	////	Multiplicacion
	c <- a * 2;
	Escribir "Multiplicacion: ", c;
	
	////	Division
	d <- b / c;
	Escribir "Division: ", d;
	
	////	Potencia ^ 
	e <- a ^ 2;
	Escribir "Potencia: ", e;
	
	////	Modulo o Residuo
	f <- 9 % 4;
	Escribir "Modulo: ", f;
	
FinProceso
